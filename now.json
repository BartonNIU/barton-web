{
    "name": "barton web",
    "version": 2,
    "builds": [
        {
            "src": "/service/app.js",
            "use": "@now/node"
        },
        {
            "src": "/client/package.json",
            "use": "@now/next"
        }
      
    ],
    "routes": [
        { "src": "/service", "dest": "app.js" },
        {
            "src": "/client",
            "dest": "/out/index.html"
        }
       
    ]
}